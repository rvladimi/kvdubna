<?xml version="1.0" encoding="utf-8"?><%@ page contentType="text/html; charset=utf-8" %><%@ page pageEncoding="utf-8" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE HTML><html><head>    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">    <link rel="shortcut icon" href="/resources/images/favicon.ico" type="image/x-icon">    <title>        Панель администратора    </title>    <style>        table {            margin: auto;        }        .bluebatton {            border-radius: 10px;            background: lightsteelblue;            color: black;            font-family: Verdana, Helvetica, Tahoma, sans-serif;            font-weight: normal;            font-size: 130%;        }        .select1 {            background: #b1ca00;            color: black;            font-family: Verdana, Helvetica, Tahoma, sans-serif;            font-weight: normal;            font-size: 22px;        }        select {            color: #363636;            font-family: Verdana, Helvetica, Tahoma, sans-serif;            font-weight: normal;            font-size: 120%;        }        h1,h2 {            color: #363636;            font-family: Verdana, Helvetica, Tahoma, sans-serif;        }    </style></head><body><table width="1200" border="0" cellspacing="0" cellpadding="10" onmousedown="return false">    <tr>        <form action="allobjects" method="GET">            <td width="50">                <button type="submit" style="border: 0; background: rgba(54, 54, 54, 0); cursor: pointer;">                    <img src="/resources/images/kvadratfull.jpg" width="100%" alt="главная страница">                </button>            </td>            <input type="hidden" name="forsale" value="true">        </form>        <form action="allobjects" method="GET">            <td valign="center" width="180">                <input type="submit" class="select1" style="cursor: pointer;" value="главная страница">            </td>            <input type="hidden" name="forsale" value="true">        </form>    </tr></table><table width="1000" border="0" cellspacing="0" cellpadding="0">    <tr>        <td colspan="2" height="10">        </td>    </tr>    <tr>        <td colspan="2">            <h1 align="center">Панель администратора</h1>        </td>    </tr>    <tr>        <td colspan="2" height="5">            <form action="admin" method="POST">                <select name="realtyType">                    <option>квартира</option>                    <option>комната</option>                    <option>коммерческая</option>                    <option>дача/участок</option>                    <option>коттедж</option>                    <option>гараж</option>                    <option>таунхаус</option>                </select>                <input type="submit" class="bluebatton" value="открыть форму для заполнения">                <input type="hidden" name="adminAction" value="openBlank">            </form>        </td>    </tr>    <tr>        <td colspan="2" height="50">        </td>    </tr>    <tr>        <td colspan="2">            <h2>Снять, активировать, редактировать или удалить объявления:</h2>        </td>    </tr>    <tr>        <td colspan="2">            <form action="admin" method="POST">                <select name="realtyType">                    <option>квартира</option>                    <option>комната</option>                    <option>коммерческая</option>                    <option>дача/участок</option>                    <option>коттедж</option>                    <option>гараж</option>                    <option>таунхаус</option>                </select>                <input type="submit" class="bluebatton" value="показать список объектов">                <input type="hidden" name="adminAction" value="showList">            </form>        </td>    </tr>    <tr>        <td colspan="2" height="50">        </td>    </tr>    <tr>        <td width="210">            <h2>Oб агентстве:</h2>        </td>        <td>            <form action="admin" method="POST">                <input type="submit" class="bluebatton" value="открыть страницу редактирования">                <input type="hidden" name="adminAction" value="aboutAgency">            </form>        </td>    </tr>    <tr>        <td colspan="2" height="50">        </td>    </tr></table></body></html>